{
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": ["/*.js"],
  "manifest_version": 2,
  "name": "webextension-target",
  "description": "webextension target test",
  "permissions": ["<all_urls>", "tabs"]
}
