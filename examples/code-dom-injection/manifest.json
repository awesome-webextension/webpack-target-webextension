{
  "manifest_version": 3,
  "name": "Web Extension webpack HMR test",
  "version": "1.0.0",
  "background": {
    "service_worker": "./dist/background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["/dist/*.txt", "/dist/*.json"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./dist/content.js"]
    }
  ],
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"]
}
